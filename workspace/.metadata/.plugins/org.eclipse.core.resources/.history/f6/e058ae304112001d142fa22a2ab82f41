public class Cat80202{
	String noise = "meos";    // 创建一个字符串“meow"并赋给noise
	int numLives = 9;         //创建一个整数numLives并赋值为9
	public Cat80202(String animalNoise) {
		noise = animalNoise;
	}
	public static void mian(String[] args) {
		Cat80202 firstCat = new Cat80202("mew");
		Cat80202 secondCat = new Cat80202("mow");
		System.out.println(firstCat.noise);
		System.out.println(secondCat.noise);
	}
}